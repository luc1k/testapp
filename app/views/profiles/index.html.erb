<h1>Listing profiles</h1>

<table>
  <tr>
    <th>User</th>
    <th>Accepted</th>
    <th>Wrong answer</th>
    <th>Timelimit</th>
    <th>Memorylimit</th>
    <th>Score</th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%=h show_username_by_id(profile.user_id) %></td>
    <td><%=h profile.accepted %></td>
    <td><%=h profile.wrong_answer %></td>
    <td><%=h profile.timelimit %></td>
    <td><%=h profile.memorylimit %></td>
    <td><%=h profile.score %></td>
    <td><%= link_to 'Show', profile %></td>
    <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
    <td><%= link_to 'Destroy', profile, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New profile', new_profile_path %>